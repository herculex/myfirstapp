<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sumugu.liubo.myfirstapp">

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MyActivity"
            android:theme="@style/Theme.AppCompat.Dialog">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MyActivity" />
        </activity>
        <activity
            android:name=".SwipeActivity"
            android:label="@string/title_activity_swipe" />
        <activity
            android:name=".ListViewExample"
            android:label="@string/title_activity_list_view_example" />
        <activity
            android:name=".MySwipes"
            android:label="@string/title_activity_my_swipes" />
        <activity
            android:name=".ListContactActivity"
            android:label="@string/title_activity_list_contact" />
        <activity
            android:name=".MyFours"
            android:label="@string/title_activity_my_fours"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".fourinone"
            android:label="@string/title_activity_fourinone"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".swipelistviewtutorial.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".pulllistview.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".customtouch.TouchInterceptActivity" />
        <activity android:name=".mytouch.MyTouch"></activity>
    </application>

</manifest>
